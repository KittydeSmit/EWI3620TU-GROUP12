package DummyWorld;

import Visualiser.Editor.*;

public class Dummies {
	
	public static void createDummy(World wor){
		int dim = wor.getDim();
		int[][] layer = new int[dim][dim];
			
		for(int i = 0; i < dim; i++){
			for(int j = 0; j < dim; j++){
				layer[i][j] = 1;
			}
		}
		
		for(int hCoord = 0; hCoord < dim; hCoord++){
			wor.getLevel()[hCoord].maze = layer;
		}
	}

}
