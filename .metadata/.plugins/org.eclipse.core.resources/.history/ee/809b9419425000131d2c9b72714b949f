package DummyWorld;

import Visualiser.Editor.*;

public class Dummies {
	
	public static void createDummy(World wor, int dim){
		
		switch(dim){
		
		case 1:
			int[][] layer1 = new int[1][1];
			
			for(int i = 0; i < dim; i++){
				for(int j = 0; j < dim; j++){
					layer1[i][j] = 1;
				}
			}
			
		case 2:
			int[][] layer2 = new int[2][2];
		}
	}

}
